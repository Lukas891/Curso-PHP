Algoritmo "semnome"

// 45) Reescreva o exercício anterior utilizando
// a estrutura ENQUANTO.

Var

   n1, n2, divi : real

Inicio

   escreval("Digite o primeiro numero:")
   leia(n1)
   enquanto (n2 = 0) faca
      escreval("Digite o segundo numero:")
      leia(n2)
   fimenquanto
   divi := n1 / n2
   escreval("O 1o numero dividido pelo 2o e:", divi)

Fimalgoritmo